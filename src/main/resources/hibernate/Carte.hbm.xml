<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.khalid.carte.model.Carte" table="carte"
		catalog="khalid">
		<id name="carteId" type="java.lang.Long">
			<column name="CARTE_ID" />
			<generator class="identity" />
		</id>
		<!-- <property name="carteCode" type="string"> <column name="CARTE_CODE" 
			length="10" not-null="true" unique="true" /> </property> -->
			
		<property name="carteName" type="string">
			<column name="CARTE_NAME" length="20" not-null="true"
				unique="true" />
		</property>
	</class>
</hibernate-mapping>
